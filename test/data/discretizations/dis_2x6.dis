# discretization
BEGIN OPTIONS
	LENGTH_UNITS METERS
    XORIGIN 1300
    YORIGIN 500
END OPTIONS

# The DIMENSIONS block is required
BEGIN DIMENSIONS
	NLAY 1
	NROW 2
	NCOL 6
END DIMENSIONS

# The GRIDDATA block is required
BEGIN GRIDDATA
	DELR
		CONSTANT 100.0
	DELC
		CONSTANT 100.0
	TOP
		CONSTANT 0.0
	BOTM
		CONSTANT -2.5
END GRIDDATA