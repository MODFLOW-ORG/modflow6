# discretization
BEGIN OPTIONS
	LENGTH_UNITS METERS
END OPTIONS

# The DIMENSIONS block is required
BEGIN DIMENSIONS
	NLAY 1
	NROW 5
	NCOL 5
END DIMENSIONS

# The GRIDDATA block is required
BEGIN GRIDDATA
	DELR
		CONSTANT 1.0
	DELC
		CONSTANT 0.2
	TOP
		CONSTANT 0.0
	BOTM
		CONSTANT -2.0
END GRIDDATA