# discretization
BEGIN OPTIONS
	LENGTH_UNITS METERS
    XORIGIN 3.0
    YORIGIN 0.0
END OPTIONS

# The DIMENSIONS block is required
BEGIN DIMENSIONS
	NLAY 1
	NROW 3
	NCOL 3
END DIMENSIONS

# The GRIDDATA block is required
BEGIN GRIDDATA
	DELR
		CONSTANT 1.0
	DELC
		CONSTANT 1.0
	TOP
		CONSTANT 0.0
	BOTM
		CONSTANT -2.0
END GRIDDATA